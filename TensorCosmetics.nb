Notebook[{
Cell[BoxData[{
 RowBox[{
  RowBox[{"BeginPackage", "[", "\"\<TensorCosmetics`\>\"", "]"}], ";"}], "\n", 
 RowBox[{
  RowBox[{"Unprotect", "@@", 
   RowBox[{"Names", "[", "\"\<TensorCosmetics`*\>\"", "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"ClearAll", "@@", 
    RowBox[{"Names", "[", "\"\<TensorCosmetics`*\>\"", "]"}]}], ";"}], 
  "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"GenInd", "::", "usage"}], "=", "\[IndentingNewLine]", 
   "\"\<GenInd[x_] is a GSH index generator that returns the index or a \
sequence of concatenated indices corresponding to the arguments x = 1 or 2 or \
3 or a list of these numbers, for 3D geometry.\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"GenerateTensor", "::", "usage"}], "=", "\[IndentingNewLine]", 
   "\"\<GenerateTensor[x_] is a tensor index generator that returns the \
elements of tensor x in tensorial form subscripted by -,0 or + depending upon \
its location in the tensor\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"UnitTensor", "::", "usage"}], " ", "=", " ", 
   "\[IndentingNewLine]", 
   "\"\<UnitTensor[A_] returns tensor with 1 in slot A and 0 everywhere else. \
Rank = Length[A]. Dim = 3.\[IndentingNewLine]Rank = 1 if Length[A] = 0 i.e. A \
is an integer\>\""}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Symmetrise", "::", "usage"}], "=", "\[IndentingNewLine]", 
    "\"\<Symmetrise[h_] symmetrises a given second order tensor.\>\""}], 
   ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Begin", "[", "\"\<`Private`\>\"", "]"}], ";"}], "\n"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"GenInd", "[", "i_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "i", "]"}], "\[Equal]", "0"}], ",", 
       RowBox[{"Switch", "[", 
        RowBox[{
        "i", ",", "1", ",", "\"\<-\>\"", ",", "2", ",", "\"\<0\>\"", ",", "3",
          ",", "\"\<+\>\""}], "]"}], ",", "\[IndentingNewLine]", 
       RowBox[{"Module", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"k", ",", "temp"}], "}"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"temp", "=", "\"\<\>\""}], ";", "\[IndentingNewLine]", 
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"k", "=", "1"}], ",", 
            RowBox[{"k", "\[LessEqual]", 
             RowBox[{"Length", "[", "i", "]"}]}], ",", 
            RowBox[{"k", "++"}], ",", 
            RowBox[{"temp", "=", 
             RowBox[{"temp", "<>", 
              RowBox[{"GenInd", "[", 
               RowBox[{"i", "[", 
                RowBox[{"[", "k", "]"}], "]"}], "]"}]}]}]}], "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"Return", "[", "temp", "]"}], ";"}]}], 
        "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", "]"}]}], 
    "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"GenerateTensor", "[", "T_", "]"}], ":=", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"i", ",", "j"}], "}"}], ",", 
      RowBox[{"Return", "[", 
       RowBox[{"Table", "[", 
        RowBox[{
         SubscriptBox[
          RowBox[{"Symbol", "[", "T", "]"}], 
          RowBox[{"GenInd", "[", 
           RowBox[{"{", 
            RowBox[{"i", ",", "j"}], "}"}], "]"}]], ",", 
         RowBox[{"{", 
          RowBox[{"i", ",", "1", ",", "3"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"j", ",", "1", ",", "3"}], "}"}]}], "]"}], "]"}]}], "]"}]}],
    ";"}], "\n"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"UnitTensor", "[", "A_", "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"ret", ",", "i"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"Max", "[", "A", "]"}], ">", "3"}], "||", 
          RowBox[{
           RowBox[{"Max", "[", "A", "]"}], "<", "0"}]}], ",", 
         RowBox[{"Return", "[", "0", "]"}]}], "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Length", "[", "A", "]"}], "\[Equal]", "0"}], ",", 
         RowBox[{"ret", "=", 
          RowBox[{"Array", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"KroneckerDelta", "[", 
              RowBox[{"#", ",", "A"}], "]"}], "&"}], ",", "3"}], "]"}]}], ",",
          "\[IndentingNewLine]", 
         RowBox[{"(", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"ret", "=", "1"}], ";", "\[IndentingNewLine]", 
           RowBox[{"For", "[", 
            RowBox[{
             RowBox[{"i", "=", "1"}], ",", 
             RowBox[{"i", "\[LessEqual]", 
              RowBox[{"Length", "[", "A", "]"}]}], ",", 
             RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"ret", "=", 
               RowBox[{"TensorProduct", "[", 
                RowBox[{"ret", ",", 
                 RowBox[{"UnitTensor", "[", 
                  RowBox[{"A", "[", 
                   RowBox[{"[", "i", "]"}], "]"}], "]"}]}], "]"}]}], ";"}]}], 
            "\[IndentingNewLine]", "]"}], ";"}], "\[IndentingNewLine]", 
          ")"}]}], "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "ret", "]"}], ";"}], "\[IndentingNewLine]", 
      ")"}]}], "\[IndentingNewLine]", "]"}]}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{
    RowBox[{"Symmetrise", "[", "h_", "]"}], ":=", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"i", ",", "j"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ret", "=", 
        RowBox[{"Evaluate", "[", "h", "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"For", "[", 
        RowBox[{
         RowBox[{"i", "=", "1"}], ",", 
         RowBox[{"i", "\[LessEqual]", "3"}], ",", 
         RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"For", "[", 
           RowBox[{
            RowBox[{"j", "=", 
             RowBox[{"i", "+", "1"}]}], ",", 
            RowBox[{"j", "\[LessEqual]", "3"}], ",", 
            RowBox[{"j", "++"}], ",", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"ret", "[", 
                RowBox[{"[", "i", "]"}], "]"}], "[", 
               RowBox[{"[", "j", "]"}], "]"}], "=", 
              RowBox[{
               RowBox[{"h", "[", 
                RowBox[{"[", "j", "]"}], "]"}], "[", 
               RowBox[{"[", "i", "]"}], "]"}]}], ";"}]}], 
           "\[IndentingNewLine]", "]"}], ";"}]}], "\[IndentingNewLine]", 
        "]"}], ";", "\[IndentingNewLine]", 
       RowBox[{"Return", "[", "ret", "]"}], ";"}]}], "\[IndentingNewLine]", 
     "]"}]}], ";"}], "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"End", "[", "]"}], ";"}], "\n", 
